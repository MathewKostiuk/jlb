{%- comment %}<locksmith:5637>{% endcomment -%}
  {%- include 'locksmith' -%}
{%- comment %}</locksmith:5637>{% endcomment -%}

<!doctype html>
<!--[if IE 9]> <html class="ie9 no-js" lang="{{ shop.locale }}" clrsheme="colorswatch1"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html{% if settings.use_boxed %} id="tt-boxed"{% endif %} class="no-js" lang="{{ shop.locale }}" clrsheme="colorswatch1"> <!--<![endif]-->
<head>
  {{ locksmith_initializations }}
  <!-- Basic page needs ================================================== -->
  <meta charset="utf-8">
  <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]-->
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta name="theme-color" content="{{ settings.color_base }}">
  <meta name="keywords" content="{{ "general.meta.keywords" | t }}" />
  <meta name="author" content="{{ "general.meta.author" | t }}">
  <link rel="canonical" href="{{ canonical_url }}">
  
  {%- if settings.use_favicon -%}
  	<link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/x-icon" />
  {%- endif -%}

  <title>Products Editor</title>
  {%- comment %}<locksmith:a8b0>{% endcomment -%}
    {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta name="description" content="Products Editor">{% endif -%}
    {%- comment %}original: <meta name="description" content="Products Editor">{%- endcomment %}
  {%- comment %}</locksmith:a8b0>{% endcomment -%}
  
  <!-- CSS ================================================== -->
  <link href="https://fonts.googleapis.com/css?family={{ settings.font_name1 }}:100,200,300,400,500,600,700,800,900" rel="stylesheet">
  {%- if settings.font_name1 != settings.font_name2 and settings.font_name2 != '' -%}
  <link href="https://fonts.googleapis.com/css?family={{ settings.font_name2 }}:100,200,300,400,500,600,700,800,900" rel="stylesheet">
  {%- endif -%}
  {{ settings.google_font_links }}
  
  {%- include "include_css" -%}
  {{ 'jquery.min.js' | asset_url | script_tag | remove: ' type="text/javascript"' }}

  <!-- Header hook for plugins ================================================== -->
  {{ content_for_header }}
  <!-- /Header hook for plugins ================================================== -->
{% render 'weglot_hreftags' %}
{% render 'weglot_switcher' %}
</head>
<body>
  <div id="tt-pageContent">
    {{ content_for_layout }}
  </div>
  <a href="#" class="tt-back-to-top">{{ 'general.general.to_top_button' | t }}</a>

  <script>

    var wokiee_app = {
      url: '{{ shop.metafields.ollistsapp.appurl }}',
      loader_text: '{{ "wishlist.buttons_text.tooltop_load_text" | t }}',
      main_info: {
        iid: '{{ shop.metafields.ollistsapp.iid }}',
        shop: '{{ shop.permanent_domain }}',
        domain: '{{ shop.domain }}',
      	lic: '{{ settings.license }}',
      }
    };

  </script>
  
  {%- include "include_js" -%}
  
</body>
</html>
